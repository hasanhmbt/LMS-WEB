@model AddOrEditEventViewModel


@{
    Layout = "_Layout";
}
@{
    ViewData["Title"] = "Add Event";
}

<!-- Page Heading -->
<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Add Event</h1>
</div>


<form method="post" asp-action="AddEvent" , asp-controller="Event" enctype="multipart/form-data">

    <partial name="_AddOrEditEventPartial" model="@Model" />

</form>

